{
    "name": "themes-go-acme",
    "displayName": "Acme and Go Themes (playground & src)",
    "description": "Themes that minics acme, play.golang.org and golang.org/src/",
    "version": "0.0.2",
    "license": "MIT",
    "publisher": "aaqaishtyaq",
    "engines": {
        "vscode": "^1.43.0"
    },
    "packageManager": "pnpm@7.5.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/aaqaishtyaq/themes-go-acme"
    },
    "files": [
        "themes"
    ],
    "bugs": {
        "url": "https://github.com/aaqaishtyaq/themes-go-acme/issues"
    },
    "icon": "icon.png",
    "preview": true,
    "contributes": {
        "themes": [
            {
                "label": "Acme",
                "uiTheme": "vs",
                "path": "./themes/acme.json"
            },
            {
                "label": "Go - Playground",
                "uiTheme": "vs",
                "path": "./themes/go-playground.json"
            },
            {
                "label": "Go - Sources",
                "uiTheme": "vs",
                "path": "./themes/go-source.json"
            }
        ]
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "theme",
        "acme",
        "light",
        "go",
        "go-playground",
        "playground",
        "golang",
        "vscode",
        "vscode-theme"
    ],
    "scripts": {
        "build": "esno src/index.ts",
        "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
        "vscode:prepublish": "npm run build",
        "lint": "eslint .",
        "package": "vsce package -o ./build/themes-go-acme-vscode.vsix",
        "release": "npm run build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
    },
    "devDependencies": {
        "eslint": "^8.20.0",
        "esno": "^0.16.3",
        "nodemon": "^2.0.19",
        "tsup": "^6.1.3",
        "typescript": "^4.7.4",
        "vsce": "^2.10.0"
    }
}
